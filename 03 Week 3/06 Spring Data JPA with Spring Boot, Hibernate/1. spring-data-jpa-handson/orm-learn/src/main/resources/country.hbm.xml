<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cognizant.ormlearn.model.Country" table="country">
        <id name="code" column="co_code" type="string"/>
        <property name="name" column="co_name" type="string"/>
    </class>
</hibernate-mapping>
